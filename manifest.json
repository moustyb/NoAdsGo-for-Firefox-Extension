{
  "manifest_version": 2,
  "name": "NoAdsGo",
  "version": "1.0",
  "description": "Simple URL blocking extension for Firefox",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "96": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "cube-adblock@mousty.dev",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
